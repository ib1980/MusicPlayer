<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicplayer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PreviousSong"
            android:label="Previous Song" />
        <activity
            android:name=".NextSongActivity"
            android:label="Next Song" />
        <activity
            android:name=".SongOneActivity"
            android:label="Song #1" />
        <activity
            android:name=".SongTwoActivity"
            android:label="Song #2" />
        <activity
            android:name=".SongThreeActivity"
            android:label="Song #3">

        </activity>
    </application>
</manifest>